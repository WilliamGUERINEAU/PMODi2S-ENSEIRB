utilities_xmdf::xmdfSetData $instance FileSet $fcount relative_path BassBoostfilt_decode_rom.mif
utilities_xmdf::xmdfSetData $instance FileSet $fcount type AnyView
incr fcount

utilities_xmdf::xmdfSetData $instance FileSet $fcount associated_module BassBoost
incr fcount

}

# ::gen_comp_name_xmdf::xmdfApplyParams
