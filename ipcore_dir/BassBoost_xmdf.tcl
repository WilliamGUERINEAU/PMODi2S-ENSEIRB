incr fcount

utilities_xmdf::xmdfSetData $instance FileSet $fcount relative_path BassBoostCOEFF_auto0_0.mif
utilities_xmdf::xmdfSetData $instance FileSet $fcount type AnyView
incr fcount

utilities_xmdf::xmdfSetData $instance FileSet $fcount relative_path BassBoost_xmdf.tcl
utilities_xmdf::xmdfSetData $instance FileSet $fcount type AnyView
